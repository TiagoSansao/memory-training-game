{
  "expo": {
    "name": "memory-training-game",
    "slug": "memory-training-game",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.exideas.cp.words.english"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    }
  }
}
